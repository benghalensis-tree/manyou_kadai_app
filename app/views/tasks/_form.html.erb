<%= form_with(model: @task) do |f|%>
  <%= f.label :task_name %>
  <%= f.text_field :task_name %>
  <%= f.label :content %>
  <%= f.text_field :content %>
  <%= f.label :status %>
  <%= f.text_field :status %>
  <%= f.label :priority %>
  <%= f.text_field :priority %>
  <%= f.label :end_date %>
  <%= f.text_field :end_date %>
  <%= f.submit %>
<% end %>